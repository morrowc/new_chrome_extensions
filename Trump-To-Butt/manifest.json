{
	"manifest_version": 2,
	"name": "Trump-To-Butt",
	"version": "0.7",
	"description": "Replaces the text 'Trump' with 'Butt'",
	"content_scripts": 
	[
		{
			"matches": ["*://*/*"],
			"js": ["content_script.js"],
			"run_at": "document_end"
		}
	],
  "options_ui": {
      "page": "options.html",
      "chrome_style": true
  }
}
